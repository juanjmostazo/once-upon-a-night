import basic_texture from BasicTexture.material
import default_changeworld from ChangeWorld.material

material woodbox_d : basic_texture
{
	set_texture_alias diffuse_texture woodbox_d.dds
}

material woodbox_n : basic_texture
{
	set_texture_alias diffuse_texture woodbox_n.dds
}

material woodbox_cw : default_changeworld
{
	set_texture_alias dreams woodbox_d.dds
	set_texture_alias nightmares woodbox_n.dds
}