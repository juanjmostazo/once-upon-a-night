import basic_texture from BasicTexture.material
import default_changeworld from ChangeWorld.material

material walldoor_d : basic_texture
{
	set_texture_alias diffuse_texture walldoor_d.dds
}

material walldoor_n : basic_texture
{
	set_texture_alias diffuse_texture walldoor_n.dds
}

material walldoor_cw : default_changeworld
{
	set_texture_alias dreams walldoor_d.dds
	set_texture_alias nightmares walldoor_n.dds
}