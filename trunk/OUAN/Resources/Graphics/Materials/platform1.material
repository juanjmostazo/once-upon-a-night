import basic_texture from BasicTexture.material
import default_changeworld from ChangeWorld.material

material platform1_d : basic_texture
{
	set_texture_alias diffuse_texture platform1_d.dds
}

material platform1_n : basic_texture
{
	set_texture_alias diffuse_texture platform1_n.dds
}

material platform_cw : default_changeworld
{
	set_texture_alias dreams platform1_d.dds
	set_texture_alias nightmares platform1_n.dds
}
