import basic_texture from BasicTexture.material
import default_changeworld from ChangeWorld.material

material clockpiece_d : basic_texture
{
	set_texture_alias diffuse_texture piece_clock.dds
}

material clockpiece_n : basic_texture
{
	set_texture_alias diffuse_texture piece_clock.dds
}

material clockpiece_cw : default_changeworld 
{
	set_texture_alias dreams piece_clock.dds
	set_texture_alias nightmares piece_clock.dds
}