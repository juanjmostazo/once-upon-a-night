import basic_texture from BasicTexture.material
import default_changeworld from ChangeWorld.material

material platform6_d : basic_texture
{
	set_texture_alias diffuse_texture platform6_d.dds
}

material platform6_n : basic_texture
{
	set_texture_alias diffuse_texture platform6_n.dds
}

material platform6_cw : default_changeworld
{
	set_texture_alias dreams platform6_d.dds
	set_texture_alias nightmares platform6_n.dds
}