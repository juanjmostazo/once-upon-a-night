import basic_texture from BasicTexture.material
import default_changeworld from ChangeWorld.material

material greatwall_d : basic_texture
{
	set_texture_alias diffuse_texture wall02_d.dds
}

material greatwall_n : basic_texture
{
	set_texture_alias diffuse_texture wall02_n.dds
}

material greatwall_cw : default_changeworld
{
	set_texture_alias dreams wall02_d.dds
	set_texture_alias nightmares wall02_n.dds
}